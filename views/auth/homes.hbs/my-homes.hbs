<h1>My Homes</h1> <br>

{{#each home}}
<div class="card mb-3" style="max-width: 540px;">
  <div class="row g-0" class="row row-cols-1 row-cols-md-3 g-4">
    <div class="col-md-4">
      <img src="{{this.image}}" class="img-fluid rounded-start" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">Title: {{this.title}}</h5>
        <br>
        <form action="/home/delete-home" method="POST">
    <input type="hidden" name="homeId" value="{{this.id}}">
    <button type='submit' class="btn btn-outline-danger">Remove from my home</button>
</form>
      </div>
    </div>
  </div>
</div>
{{/each}}
