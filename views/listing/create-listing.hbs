<div id="create-listing">
    <h3>Create a New Listing</h3>
    <form action="create-listing" method="POST">
        <label> Name
            <input type="text" name="name">
        </label>
        <label>Home
            <input type="text" name="home">
        </label>
        <label> Description
            <textarea name="description" rows="4"></textarea>
        </label>

        <button type='submit' class="btn btn-outline-dark btn-sm" type="submit">Create</button>
    </form>
</div>